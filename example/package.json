    "ionicons": "^5.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^5.1.0",
    "react-router-dom": "^5.1.0",
    "react-scripts": "^5.0.1",
    "styled-components": "^5.3.1",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --transformIgnorePatterns 'node_modules/(?!(@ionic/react|@ionic/react-router|@ionic/core|@stencil/core|ionicons)/)'",
    "eject": "react-scripts eject",
    "sync": "cd ../ && npm run build && cd ./example && ionic serve --devapp",
    "device": "cd ../ && npm run build && cd ./example && npm run build && npx cap copy",
    "ios": "cd ../ && npm run build && cd ./example && npm run build && npx cap run ios",
    "android": "cd ../ && npm run build && cd ./example && npm run build && npx cap run android"
  },
  "author": {
    "name": "Soree Choi",
    "email": "mymusese@gmail.com",
    "url": "https://github.com/SoleeChoi"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!@ionic/react|@ionic/react-router|@ionic/core|@stencil/core|ionicons)"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
